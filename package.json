{
  "name": "jecloud-cli",
  "version": "0.1.0",
  "private": true,
  "description": "JECloud 插件项目",
  "author": {
    "name": "linzhichao",
    "email": "linzc@china-hbp.com"
  },
  "scripts": {
    "dev": "npm run dev:vite",
    "build": "npm run build:vuecli",
    "dev:vuecli": "vue-cli-service serve",
    "build:vuecli": "vue-cli-service build",
    "dev:vite": "vite",
    "build:vite": "vite build",
    "preview:vite": "vite preview",
    "lint": "vue-cli-service lint",
    "setup": "npm run setup:yalc",
    "setup:yalc": "npm run yalc:remove && npm run yalc:jecloud && npm i",
    "yalc:jecloud": "yalc add @jecloud/utils @jecloud/ui",
    "yalc:remove": "yalc remove --all",
    "setup:regist": "npm i -S @jecloud/ui @jecloud/utils --regist=http://39.106.75.216:4873/ && npm i",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "rimraf node_modules",
    "clean:vite": "rimraf node_modules/.vite",
    "theme": "node build/theme/index.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@jecloud/ui": "file:.yalc/@jecloud/ui",
    "@jecloud/utils": "file:.yalc/@jecloud/utils",
    "ant-design-vue": "^3.0.0-alpha.3",
    "core-js": "^3.6.5",
    "pinia": "^2.0.0-rc.10",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.0-beta.16",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "xe-utils": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.14",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@zougt/some-loader-utils": "^1.3.4",
    "@zougt/vite-plugin-theme-preprocessor": "^1.3.12",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^4.3.8",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "vite": "^2.6.4",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^1.3.0"
  }
}
