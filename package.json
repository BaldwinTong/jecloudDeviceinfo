{
  "name": "jecloud-cli",
  "version": "1.0.0",
  "private": true,
  "description": "JECloud 插件项目",
  "author": {
    "name": "linzhichao",
    "email": "linzc@china-hbp.com"
  },
  "scripts": {
    "dev": "npm run dev:vite",
    "build": "npm run build:public && npm run build:vuecli && npm run copy:public",
    "preview": "node build/server/index.js",
    "dev:vuecli": "vue-cli-service serve",
    "build:vuecli": "vue-cli-service build",
    "build:report": "vue-cli-service build --report",
    "build:public": "node build/public/webpack.js",
    "copy:public": "gulp copy-public",
    "dev:vite": "vite",
    "build:vite": "vite build",
    "preview:vite": "vite preview",
    "lint": "vue-cli-service lint",
    "setup": "npm run setup:yalc",
    "setup:yalc": "npm run yalc:remove && npm run yalc:jecloud && pnpm i",
    "yalc:jecloud": "yalc add @jecloud/utils @jecloud/ui @jecloud/func @jecloud/plugin",
    "yalc:remove": "yalc remove --all",
    "setup:regist": "npm run regist:remove && npm run regist:jecloud && pnpm i",
    "regist:jecloud": "pnpm add @jecloud/utils @jecloud/ui @jecloud/func @jecloud/plugin",
    "regist:remove": "pnpm remove @jecloud/utils @jecloud/ui @jecloud/func @jecloud/plugin",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "rimraf node_modules",
    "clean:vite": "rimraf node_modules/.vite",
    "theme": "node build/theme/index.js",
    "config": "vue inspect > output.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@jecloud/func": "file:.yalc/@jecloud/func",
    "@jecloud/plugin": "file:.yalc/@jecloud/plugin",
    "@jecloud/ui": "file:.yalc/@jecloud/ui",
    "@jecloud/utils": "file:.yalc/@jecloud/utils",
    "ant-design-vue": "^3.2.0",
    "core-js": "^3.6.5",
    "pinia": "^2.0.0-rc.10",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.0-beta.16",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons-vue": "^6.0.1",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@zougt/some-loader-utils": "^1.3.4",
    "@zougt/theme-css-extract-webpack-plugin": "^1.4.0",
    "@zougt/vite-plugin-theme-preprocessor": "^1.4.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cors": "^2.8.5",
    "css-loader": "^3.5.2",
    "del": "^6.0.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "eslint": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-rev": "^9.0.0",
    "husky": "^4.3.8",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.21",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "1.6.2",
    "mockjs": "^1.1.0",
    "null-loader": "^4.0.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.9",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "vite": "^2.6.4",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^1.3.0",
    "webpack": "^4.0.0",
    "webpack-inject-plugin": "^1.5.5"
  }
}
