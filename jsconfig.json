{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@micro/*":["service/micro/*"],
      "@admin/*":["service/admin/*"],
      "@common/*":["service/common/*"]
    },
    "target": "ES6",
    "module": "commonjs",
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*","service/**/*","build/**/*"],
  "exclude": ["node_modules"]
}
